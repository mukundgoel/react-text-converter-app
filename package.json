{
  "name": "converter-react",
  "version": "0.0.1",
  "description": "Converter application (React)",
  "scripts": {
    "lint-client": "eslint --ext .js,.jsx -c .eslintrc-client client templates",
    "lint-client-test": "eslint --ext .js,.jsx -c .eslintrc-client-test test/client",
    "lint-server": "eslint -c .eslintrc-server server",
    "lint-server-test": "eslint -c .eslintrc-server-test test/server test/func",
    "lint": "npm run lint-client && npm run lint-client-test && npm run lint-server && npm run lint-server-test",
    "test-client": "node node_modules/karma/bin/karma start karma.conf.js",
    "test-client-ci": "node node_modules/karma/bin/karma start --browsers PhantomJS,Firefox karma.conf.coverage.js",
    "test-client-ci-win": "node node_modules/karma/bin/karma start --browsers PhantomJS,IE karma.conf.js",
    "test-client-cov": "node node_modules/karma/bin/karma start karma.conf.coverage.js",
    "test-client-dev": "node node_modules/karma/bin/karma start karma.conf.dev.js",
    "test-server-rest": "mocha --opts test/server/mocha.opts test/server/rest",
    "test-server-rest-cov": "istanbul cover --config .istanbul.server-rest.yml _mocha -- --opts test/server/mocha.opts test/server/rest",
    "test-server-unit": "mocha --opts test/server/mocha.opts test/server/spec",
    "test-server-unit-cov": "istanbul cover --config .istanbul.server-unit.yml _mocha -- --opts test/server/mocha.opts test/server/spec",
    "test-server": "npm run test-server-unit && npm run test-server-rest",
    "test-server-cov": "npm run test-server-unit-cov && npm run test-server-rest-cov",
    "test-func": "mocha --opts test/func/mocha.opts test/func/spec",
    "test-func-cov": "istanbul cover --config .istanbul.func.yml _mocha -- --opts test/func/mocha.opts test/func/spec",
    "test-func-dev": "mocha --opts test/func/mocha.dev.opts test/func/spec",
    "test": "npm run test-client && npm run test-server && npm run test-func",
    "test-ci": "npm run test-client-ci && npm run test-server-cov && npm run test-func-cov",
    "test-ci-win": "npm run test-client-ci-win && npm run test-server && echo 'TODO(36) fix Appveyor test-func'",
    "test-cov": "npm run test-client-cov && npm run test-server-cov && npm run test-func-cov",
    "test-dev": "npm run test-client-dev && npm run test-server && npm run test-func-dev",
    "check": "npm run lint && npm run test",
    "check-ci": "npm run lint && npm run test-ci",
    "check-ci-win": "npm run lint && npm run test-ci-win",
    "check-cov": "npm run lint && npm run test-cov",
    "check-dev": "npm run lint && npm run test-dev",
    "start": "node server/index.js",
    "server": "nodemon --watch client --watch server --watch templates --ext js,jsx server/index.js",
    "server-dev": "nodemon --watch client --watch server --watch templates --ext js,jsx server/index-dev.js",
    "server-hot": "nodemon --watch client --watch server --watch templates --ext js,jsx server/index-hot.js",
    "server-wds-dev": "webpack-dev-server --config webpack.config.dev.js --progress --colors --port 2992",
    "server-wds-hot": "webpack-dev-server --config webpack.config.hot.js --hot --progress --colors --port 2992 --inline",
    "server-wds-test": "webpack-dev-server --port 3001 --config webpack.config.test.js --colors",
    "sources": "http-server -p 3001 .",
    "watch": "webpack --watch --colors",
    "prod": "concurrent -kr 'npm run watch' 'npm run server' 'npm run sources'",
    "dev": "concurrent -kr 'npm run server-wds-test' 'npm run server-wds-dev' 'npm run server-dev'",
    "hot": "concurrent -kr 'npm run server-wds-test' 'npm run server-wds-hot' 'npm run server-hot'",
    "build": "webpack",
    "install-dev": "selenium-standalone install",
    "postinstall": "node heroku/scripts/not-heroku.js || (node heroku/scripts/install.js && npm run build)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/converter-react.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "example"
  ],
  "author": "Ryan Roemer <ryan.roemer@formidablelabs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/converter-react/issues"
  },
  "homepage": "https://github.com/FormidableLabs/converter-react",
  "engines": {
    "node": "0.12.x",
    "npm": "2.1.x"
  },
  "dependencies": {
    "babel": "^5.8.19",
    "babel-core": "^5.8.19",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.19",
    "bluebird": "^2.9.34",
    "bootstrap": "^3.3.5",
    "clean-webpack-plugin": "^0.1.3",
    "compression": "^1.5.2",
    "css-loader": "^0.19.0",
    "es6-promise": "^3.0.2",
    "express": "^4.13.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "isomorphic-fetch": "^2.1.1",
    "react": "^15.0.1",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.0.2",
    "react-redux": "^3.0.1",
    "redux": "^3.0.2",
    "redux-logger": "^2.0.1",
    "redux-thunk": "^1.0.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-stats-plugin": "0.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.0.10",
    "chai": "^3.2.0",
    "concurrently": "^0.1.1",
    "coveralls": "^2.11.4",
    "eslint": "^1.2.1",
    "eslint-config-defaults": "^4.2.0",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-react": "^3.2.3",
    "guacamole": "^1.1.2",
    "http-server": "^0.8.0",
    "isparta-loader": "^0.2.0",
    "istanbul": "^0.3.18",
    "jade": "^1.11.0",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.2.14",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "lodash": "^3.10.1",
    "marked": "^0.3.4",
    "mocha": "^2.2.5",
    "nodemon": "^1.4.0",
    "phantomjs": "^1.9.18",
    "react-hot-loader": "^1.2.8",
    "recluster": "^0.4.0",
    "rowdy": "^0.3.2",
    "saucelabs": "^0.1.1",
    "selenium-standalone": "^4.5.3",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.0.1",
    "webdriverio": "^3.1.0",
    "webpack-dev-server": "^1.10.1"
  }
}
